{
  "name": "blok",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "tsc": "node_modules/.bin/tsc",
    "concurrently": "node_modules/.bin/concurrently",
    "clean": "rm -rf dist",
    "build": "yarn clean && yarn tsc",
    "lint": "node_modules/.bin/tslint 'src/**/*.ts?(x)'",
    "native-link": "node_modules/.bin/react-native link",
    "upgrade": "node_modules/.bin/react-native-git-upgrade",
    "run:ios": "node_modules/.bin/react-native run-ios --simulator='iPhone X'",
    "start:ios": "yarn clean; yarn concurrently -r 'yarn tsc --watch' 'yarn run:ios'"
  },
  "dependencies": {
    "history": "4.7.2",
    "mobx": "3.3.1",
    "mobx-react": "4.3.4",
    "react": "16.0.0",
    "react-native": "0.50.1",
    "react-native-linear-gradient": "2.3.0",
    "react-router-native": "4.2.0",
    "styled-components": "2.2.3"
  },
  "devDependencies": {
    "@types/react": "16.0.20",
    "@types/react-native": "0.50.0",
    "@types/react-native-linear-gradient": "^2.3.0",
    "@types/react-router-native": "4.0.1",
    "babel-jest": "21.2.0",
    "babel-preset-react-native": "4.0.0",
    "concurrently": "3.5.0",
    "jest": "21.2.1",
    "prettier": "1.7.4",
    "react-native-git-upgrade": "^0.2.7",
    "react-test-renderer": "16.0.0",
    "tslint-config-airbnb": "5.3.0",
    "tslint-config-prettier": "1.6.0",
    "typescript": "2.6.1"
  },
  "jest": {
    "preset": "react-native"
  },
  "rnpm": {
    "assets": ["./assets/fonts"]
  }
}
